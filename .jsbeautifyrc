{
  "html": {
    "brace_style": "collapse",
    "indent_char": " ",
    "indent_scripts": "normal",
    "indent_size":2,
    "max_preserve_newlines": 0,
    "preserve_newlines": false,
    "unformatted": ["a", "sub", "sup", "b", "i", "u"],
    "wrap_line_length": 0
  },
  "js": {
    "indent_size": 2,
    "indent_char": " ",
    "indent_level": 0,
    "indent_with_tabs": true,
    "preserve_newlines": false,
    "max_preserve_newlines": 0,
    "jslint_happy": false
  },
  "sql": {
    "indent_size": 4,
    "indent_char": " ",
    "indent_level": 0,
    "indent_with_tabs": false
  },
   "beautify.onSave": ["js","json","css","scss","vue","html"]
}
